@model DatabaseCar.Data_Access_Layer.Entities.SellDocument
@{
    ViewBag.Title = "Edit";
}

<section>
    <div class="container">
        <div class="row">

            <br />

            <div class="breadcrumbs">
                <ol class="breadcrumb">
                    <li><a href="/admin/index">Админпанель</a></li>
                    <li><a href="/accountant/index">Управление товарами</a></li>
                    <li class="active">Edit sell document </li>
                </ol>
            </div>


            <h4>Are you want edit sell document @Model.Id ?</h4>

            <br />
            <h2>Sell document № @Model.Id</h2>
            <div class="col-lg-4">
                <div class="login-form">
                    <form action="/salesmanager/editSellDocument" method="post" enctype="multipart/form-data">
                        @Html.HiddenFor(m => m.Id)
                        <p>
                            @Html.LabelFor(m => m.CarId, "Car")
                            <br />
                            @Html.EditorFor(m => m.CarId)
                        </p>

                        <p>
                            @Html.LabelFor(m => m.ClientId, "Client")
                            <br />
                            @Html.EditorFor(m => m.ClientId)
                        </p>

                        <p>
                            @Html.LabelFor(m => m.EmployeeId, "Employee")
                            <br />
                            @Html.EditorFor(m => m.EmployeeId)
                        </p>

                        <p>
                            @Html.LabelFor(m => m.PaymentMethodId, "Payment method")
                            <br />
                            @Html.EditorFor(m => m.PaymentMethodId)
                        </p>

                        <p>
                            @Html.LabelFor(m => m.Price, "Price")
                            <br />
                            @Html.EditorFor(m => m.Price)

                        </p>


                        <p>
                            @Html.LabelFor(m => m.SaleDate, "Sale Date")
                            <br />
                            @Html.EditorFor(m => m.SaleDate)
                        </p>


                        <p><input type="submit" value="Отправить" /></p>


                        <br /><br />

                    </form>
                </div>
            </div>

        </div>
    </div>
</section>